{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Embryologist, clinical"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                138000
            ]
        },
        "equity": {
            "type": "float",
            "default": "",
            "title": "The equity Schema",
            "examples": [
                "0"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyhandle Schema",
            "examples": [
                "anderson-arias-morrow"
            ]
        }
    },
    "examples": [{
        "title": "Embryologist, clinical",
        "salary": 138000,
        "equity": "0",
        "companyhandle": "anderson-arias-morrow"
    }]
}